                                   owl:onProperty sosa:made_by_sensor ;
                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty sosa:observed_property ;
