                                                ecsel-dr-Planning:Planing_Purpose ;
                                       ecsel-dr-Planning:has_criterias ecsel-dr-Planning:Minimization ;
                                       ecsel-dr-Planning:has_performance_measures ecsel-dr-Planning:MinimizeBacklog ,
                                                                                  ecsel-dr-Planning:MinimizeInventoryLevel ,
                                                                                  ecsel-dr-Planning:MinimizeWipCosts ;
                                       ecsel-dr-Planning:has_planning_objects ecsel-dr-Planning:FE_Backlog ,
                                                                              ecsel-dr-Planning:FE_Inventory ,
                                                                              ecsel-dr-Planning:FE_Wip ,
                                                                              ecsel-dr-Planning:FE_X ,
                                                                              ecsel-dr-Planning:FE_Y ;
                                       ecsel-dr-Planning:has_costs "backlog=150" ,
                                                                   "inventory=15" ,
                                                                   "wip=50" ;
                                       ecsel-dr-Planning:has_planning_window "12 weeks" ;
                                       ecsel-dr-Planning:sc3:formula """sum($g in Products, $t in PlanningHorizon)--> sum($j in Frontends) (WipCosts($g, $t) * FE_Wip($j, $g, $t)) + InventoryCosts($g, $t) * FE_Inventory($g, $t) + BacklogCosts($g, $t) * FE_Backlog($g, $t);

Frontends= Frontend_1, Frontend_2;
Products= Product_1;
InventoryCosts= $(hasCosts($inventory));
BacklogCosts= $(hasCosts($backlog));
WipCosts= $(hasCosts($wip));""" .


