###  http://www.w3id.org/ecsel-dr-PMV#Join_Rule
ecsel-dr-PMV:Join_Rule rdf:type owl:Class ;
                       rdfs:subClassOf [ owl:intersectionOf ( ecsel-dr-PMV:Rule
                                                              [ rdf:type owl:Class ;
                                                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty ecsel-dr-PMV:evaluates ;
                                                                                owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass ecsel-dr-PMV:Event
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty ecsel-dr-PMV:is_assigned_to ;
                                                                                owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass ecsel-dr-ORG:Task
                                                                              ]
                                                                            )
                                                              ]
                                                              [ rdf:type owl:Class ;
                                                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty ecsel-dr-PMV:activates_task ;
                                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass ecsel-dr-ORG:Task
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty ecsel-dr-PMV:leads_to_event ;
                                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass ecsel-dr-PMV:Event
                                                                              ]
                                                                            )
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ,
                                       [ owl:intersectionOf ( ecsel-dr-PMV:Rule
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty ecsel-dr-PMV:is_assigned_to ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass ecsel-dr-ORG:Task
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty ecsel-dr-PMV:leads_to_event ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass ecsel-dr-PMV:Event
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                       owl:disjointWith ecsel-dr-PMV:Split_Rule ;
                       rdfs:comment "Rule element that unites the control flow after being split into several branches. Has several incoming paths (at least two) and one outgoing." ;
                       rdfs:label "Join Rule"@en ;
                       skos:altLabel "join rule" .


