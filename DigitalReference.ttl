                                                                 owl:onClass ecsel-dr-PMV:Event
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ecsel-dr-PMV:is_assigned_to ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass ecsel-dr-PMV:Task
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
